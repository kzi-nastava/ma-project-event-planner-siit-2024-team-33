<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31" >
        <activity
            android:name=".page.ServiceDetailsActivity"
            android:exported="false"
            android:label="@string/title_activity_service_details"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".page.activity_service_create"
            android:exported="false"
            android:label="@string/title_activity_service_create"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".page.MainPageActivity"
            android:exported="false" />
        <activity
            android:name=".page.ProfileTypeActivity"
            android:exported="false" />
        <activity
            android:name=".OrgActivityMandatory"
            android:exported="false" />
        <activity
            android:name=".OrgActivityOptional"
            android:exported="false" />
        <activity
            android:name=".page.EventPlannerActivityOptional"
            android:exported="false" />
        <activity
            android:name=".page.EventPlannerActivityMandatory"
            android:exported="false" />
        <activity
            android:name=".page.ReportsActivity"
            android:exported="false" />
        <activity
            android:name=".page.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".page.CommentsActivity"
            android:exported="false" />
        <activity
            android:name=".ProfileTypeFragment"
            android:exported="false" />
        <activity
            android:name=".page.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".page.SplashScreenActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>